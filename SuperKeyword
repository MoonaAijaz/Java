package day15;

public class Animal {
   String color = "white";
   void eat()
   {
	   System.out.println("eating....");
   }
   Animal()
   {
	   System.out.println("This is animal...");
   }
   Animal(String name)
   {
	   System.out.println(name);
   }
}
   class Dog extends Animal{
	   String color = "Black";
	   void displayColor()
	   {
		   System.out.println(super.color);
	   }
	   void eat()
	   {
		   //System.out.println("eating bread");
		   super.eat();
	   }
	   Dog()
	   {   
		   super();
		  // System.out.println("this is Dog...");
	   }
	   Dog(String name)
	   {
		   super(name);
	   }
   } 
   
	
